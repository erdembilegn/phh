    "@emotion/styled": "^11.11.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "axios": "^1.6.0",
    "framer-motion": "^10.16.4",
    "jotai": "^2.4.3",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
